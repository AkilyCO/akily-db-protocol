{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "es6"],
    "module": "commonjs",
    "outDir": "lib",
    "removeComments": true,
    "esModuleInterop": true,
    "incremental": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "declaration": true,
  },
  "include": [
    "src"
  ]
}